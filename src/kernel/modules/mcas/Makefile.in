
KERNEL_VER = `uname -r`
BUILD = `date +%Y%m%d.%k%m`
INCLUDES = -I${CMAKE_INSTALL_PREFIX}/include
LIBS = -L${CMAKE_INSTALL_PREFIX}/lib -lnupm

ccflags-y := -g -Wall

obj-m += mcasmod.o

mcasmod-objs := mcas.o page_walk.o

all: mcasmod.ko

mcasmod.ko:
	make -C /lib/modules/$(shell uname -r)/build/ M=$(PWD) modules

clean:
	make -C /lib/modules/$(shell uname -r)/build/ M=$(PWD) clean

client:
	g++ -g -std=c++11 -DCONFIG_DEBUG -O2 -o testcli ./testcli.cc $(INCLUDES)


.PHONY: client
